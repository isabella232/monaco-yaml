{
  "compilerOptions": {
    "module": "umd",
    "moduleResolution": "node",
    "outDir": "../out/amd",
    "forceConsistentCasingInFileNames": true,
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015.collection",
      "es2015.promise",
      "es2016",
      "es2017.string"
    ],
    "downlevelIteration": true
  }
}
